body {
	
    font-family: 'Teko', sans-serif;

    font-family: 'Roboto', sans-serif;

}
a , a:hover{
	text-decoration : none;
}
.devider {
	width: 100%;
	float:left;
	overflow: hidden;
}
.cs_container {
	float: left;
	width: 100%;
	padding: 0px 30px;
	display: table;
}
.extra_padding {
	padding-right: 2%;
	padding-left: 2%;
}
.display_table_cell {
	display: table-cell;
	vertical-align: middle;
}
.display_table_cell.width_10 {
	width: 10%;
}
.display_table_cell.width_90 {
	width: 90%;
	text-align: right;
}
.mobile_menu_bar {
	display: none;
}
.top_header.devider {
	text-align: right;
	padding: 12px 0px;
	border: 1px solid #ddd;
}
.top_text {
	font-size: 13px;
	font-family: "Roboto";
}
.logo {
	margin: 25px 0px;
}
.logo img {
	width: 170px;
}
