.single_diff{
	width: calc(33.33% - ( ( 4% + 4% ) * 0.3333 ) );
	float: left;
	border: 6px solid #333;
	padding: 15px;
	color: #7a7a7a;
	height: 130px;
	margin-right: 4%;
}
.single_diff_4 {
	float: left;
	border: 6px solid #333;
	padding: 15px;
	color: #7a7a7a;
	height: 210px;
	margin-right: 4%;
	width: calc(25% - ( ( 4% + 4% + 4% ) * 0.25 ) );
}
.major .single_diff_4:nth-child(4) {
	margin-right: 0px;
}
.major .single_diff:nth-child(3) {
	margin-right: 0px;
}
